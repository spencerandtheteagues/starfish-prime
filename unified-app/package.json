{
  "name": "starfish-prime-unified",
  "version": "2.0.0",
  "description": "AI-Powered Application Builder - Generate complete operational apps from single prompts",
  "private": true,
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build && npm run server:build",
    "start": "node dist/server/index.js",
    "server:dev": "tsx server/index.ts",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server --sourcemap",
    "client:dev": "next dev -p 3001",
    "client:build": "next build",
    "client:start": "next start -p 3001",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/websocket": "^10.0.1",
    "@monaco-editor/react": "^4.6.0",
    "@next/font": "^14.2.8",
    "@octokit/rest": "^20.1.1",
    "@prisma/client": "^5.16.1",
    "axios": "^1.7.7",
    "bullmq": "^5.12.6",
    "dockerode": "^4.0.2",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "monaco-editor": "^0.50.0",
    "nanoid": "^5.0.7",
    "next": "^14.2.8",
    "next-auth": "^4.24.7",
    "openai": "^4.56.0",
    "pino": "^9.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "ws": "^8.17.0",
    "yjs": "^13.6.18",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.12",
    "concurrently": "^8.2.2",
    "esbuild": "^0.23.1",
    "eslint": "^8.57.0",
    "prisma": "^5.16.1",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}