{
  "name": "silverguard-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for SilverGuard ElderCare",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "expo": "~50.0.17",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^4.9.0",
    "openai": "^6.16.0",
    "react": "18.2.0",
    "react-native": "0.73.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
