{
  "id": null,
  "uid": "service-deps-spanmetrics",
  "title": "Service Dependency \u2014 SpanMetrics",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "type": "table",
      "title": "Top Callers \u2192 Callees (5m rate)",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (service_name, peer_service) (rate(traces_spanmetrics_calls[5m]))",
          "format": "table",
          "instant": false
        }
      ],
      "options": {
        "showHeader": true
      }
    },
    {
      "type": "timeseries",
      "title": "Latency p95 by Service Pair",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, service_name, peer_service) (rate(traces_spanmetrics_latency_bucket[5m])))",
          "legendFormat": "{{service_name}} \u2192 {{peer_service}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}